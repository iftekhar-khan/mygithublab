# Print function
print("The Value of i is",i)
j = 20
print("The Value of j is",j)
a = i + j
print("The Sum of i and j is",a)
s = i - j
print("The Difference of i and j is",s)
m = i * j
print("The Product of i and j is",m)
d = j / i
print("The result of dividing j by i is",d)

# Use of input function.

print("Please Enter value for i")
i = input()
print("The Value you entered is",i)

print("Please Enter value for j")
j = input()
print("The Value you entered is",j)

z = int(i) + int(j)

print("The Sum is",z)
